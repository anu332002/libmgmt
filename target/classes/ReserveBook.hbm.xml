<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="edu.neu.user.ReserveBook" table="reservebook">
	    <id name="reservebookId" type="java.lang.Long">
	        <column name="reservebookId" />
			<generator class="native" />
	    </id>
	    <property name="reserveDate" type="java.util.Date">
	        <column name="reserveDate"/>
	    </property>
	    <property name="takedueDate" type="java.util.Date">
	        <column name="takedueDate"/>
	    </property>
	    <property name="booksId" type="java.lang.Long">
	        <column name="booksId"/>
	    </property>
	    <property name="usersId" type="java.lang.Long">
	        <column name="usersId"/>
	    </property>
	    <property name="status" type="java.lang.Integer" length="11">
	        <column name="status"/>
	    </property>
	</class>
</hibernate-mapping>